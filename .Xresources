#define background_opacity  92
#include ".Xresources.d/solarized.dark"
!#include ".Xresources.d/solarized.light"

URxvt.intensityStyles:      false
URxvt.geometry:             170x81

!URxvt*background:          [92]S_base03
URxvt*scrollBar:            false
