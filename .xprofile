# setxkbmap -option caps:super no && killall xcape 2>/dev/null ; xcape -e 'Super_L=Escape' &
setxkbmap \
    -model pc105 \
    -layout us \
    -variant altgr-intl \
    -option ctrl:nocaps
xcape -e 'Control_L=Escape'
xset r rate 300 50
~/.fehbg &
# ~/.local/bin/i3cmds/blur-lock &
pgrep sxhkd || sxhkd &
unclutter &
